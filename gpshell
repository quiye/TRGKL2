set logscale y
set datafile separator ","
set term pdfcairo enhanced size 4in, 3in
set output "gnuplot-pdf.pdf"
plot "1.csv" using 2:4 w l lw 3
replot "2.csv" using 2:4 w l lw 3
replot "3.csv" using 2:4 w l lw 3
replot "4.csv" using 2:4 w l lw 3
replot "5.csv" using 2:4 w l lw 3
replot "6.csv" using 2:4 w l lw 3
